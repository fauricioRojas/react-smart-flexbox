{
  "name": "bundlefi-packages",
  "version": "1.0.5",
  "description": "Bundlefi packages for React with Typescript + JSX Harmony syntax",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "author": "First Factory - Bundlefi development team",
  "scripts": {
    "build": "tsc",
    "clean-modules": "rimraf ./node_modules ./package-lock.json",
    "prettier": "prettier --write src/**/*.{ts,tsx}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && git add ."
    }
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "babel-loader": "8.0.6",
    "husky": "3.0.8",
    "path": "0.12.7",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.1.0",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "@types/node": "12.7.10",
    "@types/react": "16.9.4",
    "react": "16.10.1"
  }
}
